<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">

    <link href="https://fonts.googleapis.com/css2?family=Fira+Sans:wght@700&family=Lato&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@700&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Bebas+Neue">

    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/js/bootstrap.bundle.min.js">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/boxicons@latest/css/boxicons.min.css">

    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">
    <link href="/css/nav.css" rel="stylesheet">
    <link href="/css/form.css" rel="stylesheet">
    <link href="/css/dash.css" rel="stylesheet">


    <title>Dashboard</title>
</head>

<body id="body-pd" style="background-color: #E7E4E7;">
    
    
<header class="header" id="header" style="background-color: #E7E4E7;">
    <div class="header_toggle"> <i class='bx bx-menu' id="header-toggle"></i> </div>
    
</header>
<div class="l-navbar" id="nav-bar">
    <nav class="nav">
        <div>
            <a href="#" class="nav_logo"> <i class='bx bx-square nav_logo-icon'></i> <span
                    class="nav_logo-name">MARCO</span> </a>
            <div class="nav_list">
                <a href="/Dashboard" class="nav_link active"> <i class='bx bx-grid-alt nav_icon'></i> <span class="nav_name">Dashboard</span></a>
                <a href="/VisitaGuiada" class="nav_link"> <i class='bx bx-calendar nav_icon'></i><span
                        class="nav_name">Visitas guiadas</span> </a>
                <a href="/Capsulas" class="nav_link"> <i class='bx bx-bullseye nav_icon'></i> <span
                        class="nav_name">Capsulas</span> </a>
                <a href="/Exposiciones" class="nav_link"> <i class='bx bxs-photo-album nav_icon'></i> <span
                        class="nav_name">Exposiciones</span> </a>

            </div>
        </div>
        <a href="#" class="nav_link"> <i class='bx bx-log-out nav_icon'></i> <span class="nav_name">SignOut</span>
        </a>
    </nav>
</div>

    <!--Container Main start-->
    <div class="height-100 bg-light">
        <div class="container-fluid" style="background-color: #E7E4E7;">
            <div class="d-sm-flex justify-content-between align-items-center mb-4">
                <h3 class="text-dark mb-0">Bienvenido!</h3>
            </div>
            <div class="row">
                <div class="col-lg-7 col-xl-8">
                    <div class="card shadow mb-4">
                        <div class="card-header d-flex justify-content-between align-items-center">
                            <h6 class="text-primary fw-bold m-0">Visitas Reservadas</h6>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive table mt-2" id="dataTable" role="grid" aria-describedby="dataTable_info">
                                <table class="table my-0" id="dataTable">
                                    <thead>
                                        <tr>
                                            <th>Reservador</th>
                                            <th>Fecha</th>
                                            <th>Hora</th>
                                            <th>Guia</th>
                                            <th>#Personas</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <% data.Rguias.forEach(function(data, index){ %>
                                            <td><%- data.reservador%></td>
                                            <td><%- data.fecha%></td>
                                            <td><%- data.hora%></td>
                                            <td><%- data.Nguia%><br></td>
                                            <td><%- data.Npersonas%></td>
                                        </tr>
                                        <%})%>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-5 col-xl-4">
                    <div class="card shadow mb-4">
                        <div class="card-header d-flex justify-content-between align-items-center">
                            <h6 class="text-primary fw-bold m-0">Comparaci√≥n</h6>
                        </div>
                        <div class="card-body">
                            <div class="chart-area"><canvas
                                    data-bss-chart="{&quot;type&quot;:&quot;doughnut&quot;,&quot;data&quot;:{&quot;labels&quot;:[&quot;Visitas Disponibles&quot;,&quot;Visitas Reservadas&quot;],&quot;datasets&quot;:[{&quot;label&quot;:&quot;&quot;,&quot;backgroundColor&quot;:[&quot;#D93899&quot;,&quot;#E5E0CF&quot;],&quot;borderColor&quot;:[&quot;#ffffff&quot;,&quot;#ffffff&quot;,&quot;#ffffff&quot;],&quot;data&quot;:[&quot;<%= Ndisponibles%>&quot;,&quot;<%=Nreservas%>&quot;]}]},&quot;options&quot;:{&quot;maintainAspectRatio&quot;:true,&quot;legend&quot;:{&quot;display&quot;:false},&quot;title&quot;:{}}}"></canvas>
                            </div>
                            <div class="text-center small mt-4"><span class="me-2"><i class="bx bxs-circle"
                                        style="color: #D93899;"></i>&nbsp;Disponibles</span><span class="me-2"><i class="bx bxs-circle"
                                        style="color: #E5E0CF;"></i>&nbsp;Reservadas</span></div>
                        </div>
                    </div>
                </div>

                <div class="col-lg-7 col-xl-8">
                    <div class="card shadow mb-4">
                        <div class="card-header d-flex justify-content-between align-items-center">
                            <h6 class="text-primary fw-bold m-0">Visitas Disponibles</h6>
                        </div>
                        <div class="card-body">
                            <div class="table-responsive table mt-2" id="dataTable" role="grid" aria-describedby="dataTable_info">
                                <table class="table my-0" id="dataTable">
                                    <thead>
                                        <tr>
                                            <th>Guia</th>
                                            <th>Fecha</th>
                                            <th>Hora</th>
                                            <th>#Personas</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <% datas2.RNguias.forEach(function(data2, index){ %>
                                                <td><%- data2.Nguia%><br></td>
                                                <td><%- data2.fecha%></td>
                                                <td><%- data2.hora%></td>
                                                <td><%- data2.Npersonas%></td>
                                        </tr>
                                        <%})%>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
                
            </div>
        </div>
        <script ></script>
        <script src="/js/chart.min.js"></script>
        <script src="/js/bs-init.js"></script>
        <script src="/js/nav.js"></script>
        <script src="/js/theme.js"></script>
        <script src="/js/bootstrap.min.js"></script>

</body>

</html>
